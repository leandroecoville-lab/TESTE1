        {
          "type": "meetcore.call.started",
          "source": "lai.meetcore",
          "sample_data": {
            "call_id": "call-demo",
            "room_id": "room-demo",
            "participants": [
              {
                "id": "lead-1",
                "role": "lead"
              },
              {
                "id": "seller-1",
                "role": "seller"
              }
            ]
          }
        },
        {
          "type": "meetcore.transcript.partial",
          "source": "lai.meetcore",
          "sample_data": {
            "call_id": "call-demo",
            "seq": 1,
            "text": "Olá, como posso ajudar?",
            "confidence": 0.86
          }
        },
        {
          "type": "meetcore.call.ended",
          "source": "lai.meetcore",
          "sample_data": {
            "call_id": "call-demo",
            "duration_s": 62
          }
        },
        {
          "type": "meetcore.postcall.completed",
          "source": "lai.meetcore",
          "sample_data": {
            "call_id": "call-demo",
            "artifacts": [
              "transcript",
              "summary",
              "topics"
            ]
          }
        }
      ],
      "references": [
        {
          "name": "MeetCore Blueprint",
          "path": "docs/references/25.pdf"
        }
      ]
    },
