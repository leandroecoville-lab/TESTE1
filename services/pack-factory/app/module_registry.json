      "slices": {
        "ids": [
          "PEC1.01",
          "PEC1.02",
          "PEC1.03",
          "PEC1.04",
          "PEC1.05"
        ],
        "note": "MeetCore-first slicing: iniciar pelo PEC1.01 (Signaling+Rooms+Tokens) e evoluir até PEC1.05.",
        "template_file": "MEETCORE_SLICES.md"
      },
      "budgets": {
        "streaming_p95_ms": 300,
        "insights_p95_ms": 1000,
        "transcript_p95_ms": 2000,
        "template_file": "PERFORMANCE_BUDGETS.md"
      },
      "content_checks": [
        {
          "file": "docs/MEETCORE_SLICES.md",
          "contains": "pec1.01",
          "error": "missing_trace::meetcore_slices_PEC1_01_not_found"
        },
        {
          "file": "docs/PERFORMANCE_BUDGETS.md",
          "contains": "300ms",
          "error": "missing_trace::meetcore_budget_streaming_300ms_not_found"
        },
        {
          "file": "docs/DATA_RETENTION_MATRIX.md",
          "contains_all": [
            "culture",
            "people",
            "pipeline"
          ],
          "error": "missing_trace::retention_matrix_culture_people_not_found"
        }
      ],
